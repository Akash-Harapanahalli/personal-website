  author =	 {F. {Forni} and R. {Sepulchre}},
  journal =	 {IEEE Transactions on Automatic Control},
  title =	 {Differentially Positive Systems},
  year =	 2016,
  volume =	 61,
  number =	 2,
  pages =	 {346-359},
  doi =		 {10.1109/TAC.2015.2437523},
  abstract =	 {The paper introduces and studies differentially positive
                  systems, that is, systems whose linearization along an
                  arbitrary trajectory is positive. A generalization of
                  Perron-Frobenius theory is developed in this differential
                  framework to show that the property induces a conal order
                  that strongly constrains the asymptotic behavior of
                  solutions. The results illustrate that behaviors
                  constrained by local order properties extend beyond the
                  well-studied class of linear positive systems and
                  monotone systems, which both require a constant cone
                  field and a linear state space.}
}

